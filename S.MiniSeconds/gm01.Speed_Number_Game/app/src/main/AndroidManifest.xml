<?xml version="1.0" encoding="utf-8"?>
<manifest package="game.speed.android.speed_number_game"
          xmlns:android="http://schemas.android.com/apk/res/android">
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
    
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="game.speed.android.speed_number_game.GameSelect"
            android:label="@string/title_activity_game_select" >
        </activity>
    
        <activity
            android:name="game.speed.android.speed_number_game.Play"
            android:label="@string/title_activity_play"
            android:theme="@style/AppThemeWithActionBar" >
        </activity>
    
        <activity
            android:name="game.speed.android.speed_number_game.FinishScreen"
            android:label="@string/title_activity_finish_screen" >
        </activity>
    
        <activity
            android:name="game.speed.android.speed_number_game.HighScores"
            android:label="@string/title_activity_high_scores" >
        </activity>
    
        <activity
            android:name="game.speed.android.speed_number_game.Help"
            android:label="@string/title_activity_help" >
        </activity>
        
        
    </application>

</manifest>